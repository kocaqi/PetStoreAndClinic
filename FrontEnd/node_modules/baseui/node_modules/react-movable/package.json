{
  "name": "react-movable",
  "version": "2.5.4",
  "description": "Drag and drop lists.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tajo/react-movable"
  },
  "author": "Vojtech Miksu <vojtech@miksu.cz>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && tsc --outDir lib && cp flowtypes/* lib && rm -rf lib/examples && mv lib/src/* lib && rm -rf lib/src && rm lib/*.test.* && rm lib/*.stories.*",
    "jest:e2e": "jest --runInBand",
    "jest:e2e:update": "jest --runInBand -u e2e",
    "ladle:ci": "ladle serve --output stream --open none",
    "test": "yarn typecheck && yarn test:e2e && yarn test:unit",
    "test:e2e": "start-server-and-test ladle:ci http-get://localhost:61000 jest:e2e",
    "test:e2e:dev": "E2E=dev yarn jest:e2e",
    "test:e2e:update": "start-server-and-test ladle:ci http-get://localhost:61000 jest:e2e:update",
    "test:unit": "jest src",
    "typecheck": "tsc --noEmit"
  },
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-dom",
    "react-component",
    "sortable",
    "reorder",
    "reorderable",
    "drag",
    "drop",
    "list",
    "table",
    "drag and drop",
    "dnd",
    "movable"
  ],
  "devDependencies": {
    "@ladle/react": "0.0.11",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "^26.0.20",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/jest-image-snapshot": "^4.1.3",
    "@types/node": "^14.14.22",
    "@types/puppeteer": "^5.4.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "imgur": "^0.3.1",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.3.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "start-server-and-test": "^1.11.7",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^16.3.0-0 || ^17.0.0-0",
    "react-dom": "^16.3.0-0 || ^17.0.0-0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/e2e/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "reporters": [
      "default",
      "<rootDir>/image-reporter.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-puppeteer"
  },
  "volta": {
    "node": "14.15.4",
    "yarn": "1.22.10"
  }
}
